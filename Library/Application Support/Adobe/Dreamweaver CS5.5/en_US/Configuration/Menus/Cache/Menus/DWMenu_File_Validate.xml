<menus xmlns:MMString="http://www.macromedia.com/schemes/data/string/"><menuitem name="As _XML" file="Menus/MM/ValidateXML.htm" id="DWMenu_MainSite_File_Validate_XML" /><menuitem id="menus/DWMenu_File_Validate_CurrentDocument" name="_Validate Current Document (W3C)" enabled="(dw.getDocumentDOM() != null && (dw.getDocumentDOM().getParseMode() == 'html' || dw.getDocumentDOM().getParseMode() == 'xml') && dw.getActiveWindow() && (dw.getDocumentDOM().isLiveCodeViewOpen() || !dw.getDocumentDOM().isRelatedFileViewOpen()))" command="if(dw.getDocumentDOM().getServerNameFromDoc() == 'XSLT'){dw.getDocumentDOM().runValidation('xml');}else{dw.getDocumentDOM().runValidation('',true,true,true)}" /><menuitem id="menus/DWMenu_File_Validate_LiveDocument" name="Validate Live Document (W3C)" enabled="(dw.getDocumentDOM() != null && (dw.getDocumentDOM().getParseMode() == 'html' || dw.getDocumentDOM().getParseMode() == 'xml')) && dw.getDocumentDOM().doesCachedFileExist() && dw.getActiveWindow() && dw.getActiveWindow().getDesignViewMode() == 'live'"  command="dw.getDocumentDOM().runValidation('live',true,true,true)" />