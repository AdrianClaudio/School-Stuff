<menus xmlns:MMString="http://www.macromedia.com/schemes/data/string/"><menuitem id="DWValidateTabStart_CurDocument" name="_Validate Current Document (W3C)" enabled="(dw.getDocumentDOM() != null && (dw.getDocumentDOM().getParseMode() == 'html' || dw.getDocumentDOM().getParseMode() == 'xml') && dw.getActiveWindow() && (dw.getDocumentDOM().isLiveCodeViewOpen() || !dw.getDocumentDOM().isRelatedFileViewOpen()))" command="if(dw.getDocumentDOM().getServerNameFromDoc() == 'XSLT'){dw.getDocumentDOM().runValidation('xml');}else{dw.getDocumentDOM().runValidation('',true,true,true)}" /><menuitem id="DWValidateTabStart_LiveDocument" name="Validate Live Document (W3C)" enabled="(dw.getDocumentDOM() != null && (dw.getDocumentDOM().getParseMode() == 'html' || dw.getDocumentDOM().getParseMode() == 'xml')) && dw.getDocumentDOM().doesCachedFileExist() && dw.getActiveWindow() && dw.getActiveWindow().getDesignViewMode() == 'live'"  command="dw.getDocumentDOM().runValidation('live',true,true,true)" /><menuitem id="DWValidateTab_Settings" name="Settings..." enabled="true" command="dw.showPreferencesDialog('validator')" domRequired="false" />