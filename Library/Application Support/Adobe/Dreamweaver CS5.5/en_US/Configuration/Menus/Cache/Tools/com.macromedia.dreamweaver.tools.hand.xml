<menus xmlns:MMString="http://www.macromedia.com/schemes/data/string/"><activate name="Switch to Hand tool" key="H" id="DWTools_Hand_Active1" /><activate name="Switch to Hand tool" key="Shift+Cmd+Opt+H" id="DWTools_Hand_Active2" /><override name="Quick switch to Hand tool" key="Space" id="DWTools_Hand_Override" /><action name="Zoom In" key="=" command="dw.zoomIn()" id="DWTools_Hand_ZoomIn" /><action name="Zoom Out" key="-" command="dw.zoomOut()" id="DWTools_Hand_ZoomOut" /><action name="Set magnification to 50%" key="5" command="dw.activeViewScale = 0.50" id="DWTools_Hand_50" /><action name="Set magnification to 100%" key="1" command="dw.activeViewScale = 1.0" id="DWTools_Hand_100" /><action name="Set magnification to 200%" key="2" command="dw.activeViewScale = 2.0" id="DWTools_Hand_200" /><action name="Set magnification to 300%" key="3" command="dw.activeViewScale = 3.0" id="DWTools_Hand_300" /><action name="Set magnification to 400%" key="4" command="dw.activeViewScale = 4.0" id="DWTools_Hand_400" /><action name="Set magnification to 800%" key="8" command="dw.activeViewScale = 8.0" id="DWTools_Hand_800" /><action name="Set magnification to 1600%" key="6" command="dw.activeViewScale = 16.0" id="DWTools_Hand_1600" /><action name="Fit selection" key="S" command="if (dw.canFitSelection()) { dw.fitSelection() }" id="DWTools_Hand_FitSelection" /><action name="Fit all" key="A" command="if (dw.canFitSize()) { dw.fitAll() }" id="DWTools_Hand_FitAll" /><action name="Fit width" key="W" command="if (dw.canFitSize()) { dw.fitWidth() }" id="DWTools_Hand_FitWidth" />