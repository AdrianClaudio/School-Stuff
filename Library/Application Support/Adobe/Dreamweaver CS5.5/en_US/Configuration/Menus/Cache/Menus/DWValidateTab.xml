<menus xmlns:MMString="http://www.macromedia.com/schemes/data/string/"><menuitem id="DWValidateTab_OpenInEditor" name="Go To Line" enabled="dw.resultsPalette.canOpenInEditor();" command="dw.resultsPalette.openInEditor()" domRequired="false" /><menuitem id="DWValidateTab_AddToFilter" name="Hide Error" enabled="dw.resultsPalette.canOpenInEditor();" command="dw.resultsPalette.addToFilter()" domRequired="false" /><menuitem id="DWValidateTab_EnableFilter" name="Show All" checked="!dw.resultsPalette.isFilterEnabled()" command="dw.resultsPalette.enableFilter()" domRequired="false" /><menuitem id="DWValidateTab_MoreInfo" name="More Info..." enabled="dw.resultsPalette.canDetailInfo();" command="dw.resultsPalette.detailInfo()" domRequired="false" /><separator /><menuitem id="DWValidateTab_OpenInBrowser" name="Open Results in Browser" enabled="dw.resultsPalette.canOpenInBrowser();" command="dw.resultsPalette.openInBrowser()" domRequired="false" /><menuitem id="DWValidateTab_Save" name="Save Results..." enabled="dw.resultsPalette.canSave();" command="dw.resultsPalette.save()" domRequired="false" /><menuitem id="DWValidateTab_Clear" name="Clear Results" enabled="dw.resultsPalette.canClear();" command="dw.resultsPalette.clear()" domRequired="false" /><separator /><menuitem id="DWValidateTab_CurDocument" name="_Validate Current Document (W3C)" enabled="(dw.getDocumentDOM() != null && (dw.getDocumentDOM().getParseMode() == 'html' || dw.getDocumentDOM().getParseMode() == 'xml') && dw.getActiveWindow() && (dw.getDocumentDOM().isLiveCodeViewOpen() || !dw.getDocumentDOM().isRelatedFileViewOpen()))" command="if(dw.getDocumentDOM().getServerNameFromDoc() == 'XSLT'){dw.getDocumentDOM().runValidation('xml');}else{dw.getDocumentDOM().runValidation('',true,true,true)}" /><menuitem id="DWValidateTabStart_LiveDocument" name="Validate Live Document (W3C)" enabled="(dw.getDocumentDOM() != null && (dw.getDocumentDOM().getParseMode() == 'html' || dw.getDocumentDOM().getParseMode() == 'xml')) && dw.getDocumentDOM().doesCachedFileExist() && dw.getActiveWindow() && dw.getActiveWindow().getDesignViewMode() == 'live'"  command="dw.getDocumentDOM().runValidation('live',true,true,true)" /><menuitem id="DWValidateTab_Settings" name="Settings..." enabled="true" command="dw.showPreferencesDialog('validator')" domRequired="false" />