<menus xmlns:MMString="http://www.macromedia.com/schemes/data/string/"><activate name="Switch to Zoom tool" key="Z" id="DWTools_Zoom_Activate1" /><activate name="Switch to Zoom tool" key="Shift+Cmd+Opt+Z" id="DWTools_Zoom_Activate2" /><override name="Quick switch to Zoom tool" key="Cmd+Space" id="DWTools_Zoom_Override" /><action name="Zoom In" key="=" command="dw.zoomIn()" id="DWTools_Zoom_ZoomIn" /><action name="Zoom Out" key="-" command="dw.zoomOut()" id="DWTools_Zoom_ZoomOut" /><action name="Set magnification to 50%" key="5" command="dw.activeViewScale = 0.50" id="DWTools_Zoom_50" /><action name="Set magnification to 100%" key="1" command="dw.activeViewScale = 1.0" id="DWTools_Zoom_100" /><action name="Set magnification to 200%" key="2" command="dw.activeViewScale = 2.0" id="DWTools_Zoom_200" /><action name="Set magnification to 300%" key="3" command="dw.activeViewScale = 3.0" id="DWTools_Zoom_300" /><action name="Set magnification to 400%" key="4" command="dw.activeViewScale = 4.0" id="DWTools_Zoom_400" /><action name="Set magnification to 800%" key="8" command="dw.activeViewScale = 8.0" id="DWTools_Zoom_800" /><action name="Set magnification to 1600%" key="6" command="dw.activeViewScale = 16.0" id="DWTools_Zoom_1600" /><action name="Fit selection" key="S" command="if (dw.canFitSelection()) { dw.fitSelection() }" id="DWTools_Zoom_FitSelection" /><action name="Fit all" key="A" command="if (dw.canFitSize()) { dw.fitAll() }" id="DWTools_Zoom_FitAll" /><action name="Fit width" key="W" command="if (dw.canFitSize()) { dw.fitWidth() }" id="DWTools_Zoom_FitWidth" />