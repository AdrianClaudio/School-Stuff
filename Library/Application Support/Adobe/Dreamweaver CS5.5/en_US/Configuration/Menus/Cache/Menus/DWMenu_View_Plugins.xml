<menus xmlns:MMString="http://www.macromedia.com/schemes/data/string/"><menuitem name="_Play" key="Cmd+Opt+P" command="dw.getDocumentDOM().playPlugin()" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM().canPlayPlugin()" id="DWMenu_View_Plugins_Play" /><menuitem name="_Stop" key="Cmd+Opt+X" command="dw.getDocumentDOM().stopPlugin()" enabled="dw.getFocus() == 'document' && dw.getDocumentDOM().canStopPlugin()" id="DWMenu_View_Plugins_Stop" /><separator /><menuitem name="Play _All" key="Cmd+Opt+Shift+P" command="dw.getDocumentDOM().playAllPlugins()" enabled="dw.getFocus() == 'document' && dw.activeViewScale == 1.0" id="DWMenu_View_Plugins_PlayAll" /><menuitem name="S_top All" key="Cmd+Opt+Shift+X" command="dw.getDocumentDOM().stopAllPlugins()" enabled="dw.getFocus() == 'document'" id="DWMenu_View_Plugins_StopAll" />